void g_fnCliPrintCli(TreeCli * _pclsCli,const char * _pszBuff)
{
	if(_pclsCli->m_eMod == E_CLI_MOD_NONE || _pclsCli->m_eMod == E_CLI_MOD_CONSOL)
	{
		printf(_pszBuff);
	}
	else if(_pclsCli->m_eMod == E_CLI_MOD_CLIENT)
	{
		_pclsCli->m_fnSendToSrv(_pszBuff);
	}
}
