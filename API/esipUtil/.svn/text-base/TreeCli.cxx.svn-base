		g_fnCliOS_Command("%s > %s",(KCSTR)clsOsCmd,(KCSTR)clsTmpPath);
		//==========> Cmd Return File Load
		CfgFileInfo clsTmpFile; clsTmpFile.m_fnInit((KCSTR)clsTmpPath);
